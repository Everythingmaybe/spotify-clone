{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "dist/apps/server/open/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/apps/server/open/main.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/apps/client/open/index.html"
    }
  ]
}
